<ul class="list-inline">
    {%- for action in record_actions %}
        <li>
            <a class="btn btn-primary" href="{{ "{{ path('" ~ route_name_prefix ~ "_" ~ action ~ "', { 'id': entity."~ identifier ~" }) }}" }}">
                {{ action }}
            </a>
        </li>
    {%- endfor %}
</ul>
